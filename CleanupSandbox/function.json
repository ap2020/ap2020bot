{
  "bindings": [
    {
      "name": "timer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */20 * * * *",
      "runOnStartup": false
    },
    {
      "name": "lastDumpedMessage",
      "type": "blob",
      "path": "ap2020bot/DumpSandbox",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    }
  ],
  "scriptFile": "../dist/CleanupSandbox/index.js"
}
