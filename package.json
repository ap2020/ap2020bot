    "@types/jest": "^25.2.1",
    "@types/moment-timezone": "^0.5.13",
    "jest": "^25.5.3",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.8.1",
    "typescript": "^3.3.3"
